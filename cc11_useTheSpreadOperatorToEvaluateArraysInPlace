const arr1 = ['JAN', 'FEB', 'MAR', 'APR', 'MAY'];
let arr2;
(function() {
  "use strict";
  arr2 = [...arr1]; // change this line
})();
console.log(arr2);

//'...arr1' here is an example of the 'spread operator'
//The array 'arr1' is copied into the empty array and assigned to the variable 'arr2'
//The purpose of the spread operator is to 'unpack' the array, or 'spread' it, like a hand of playing cards fanned out in your hand
